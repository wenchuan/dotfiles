# This file set up shell variables

export EDITOR=vim
export PYTHONSTARTUP=~/.pythonrc
export PYTHONPATH=$PYTHONPATH:.
export PATH=$PATH:~/bin

autoload -U colors && colors
export PROMPT="[%{$fg[blue]%}%D %{$reset_color%}%{$fg[cyan]%}%*%{$reset_color%}] %{$fg[red]%}%~%{$reset_color%} %% "

# vim: set ft=zsh
